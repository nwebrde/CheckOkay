{
  "cli": {
    "version": ">= 10.0.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://app.checkokay.com/api/trpc",
        "EXPO_PUBLIC_OAUTH_AUTHORIZATION_ENDPOINT": "https://app.checkokay.com/api/oauth2/authorize",
        "EXPO_PUBLIC_OAUTH_TOKEN_ENDPOINT": "https://app.checkokay.com/api/oauth2/token",
        "EXPO_PUBLIC_OAUTH_REVOCATION_ENDPOINT": "https://app.checkokay.com/api/oauth2/token/revoke",
        "EXPO_PUBLIC_OAUTH_CLIENT_ID": "BeingWellApp"
      }
    },
    "preview-simulator": {
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://app.checkokay.com/api/trpc",
        "EXPO_PUBLIC_OAUTH_AUTHORIZATION_ENDPOINT": "https://app.checkokay.com/api/oauth2/authorize",
        "EXPO_PUBLIC_OAUTH_TOKEN_ENDPOINT": "https://app.checkokay.com/api/oauth2/token",
        "EXPO_PUBLIC_OAUTH_REVOCATION_ENDPOINT": "https://app.checkokay.com/api/oauth2/token/revoke",
        "EXPO_PUBLIC_OAUTH_CLIENT_ID": "BeingWellApp"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "https://app.checkokay.com/api/trpc",
        "EXPO_PUBLIC_OAUTH_AUTHORIZATION_ENDPOINT": "https://app.checkokay.com/api/oauth2/authorize",
        "EXPO_PUBLIC_OAUTH_TOKEN_ENDPOINT": "https://app.checkokay.com/api/oauth2/token",
        "EXPO_PUBLIC_OAUTH_REVOCATION_ENDPOINT": "https://app.checkokay.com/api/oauth2/token/revoke",
        "EXPO_PUBLIC_OAUTH_CLIENT_ID": "BeingWellApp"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
    }
  }
}
